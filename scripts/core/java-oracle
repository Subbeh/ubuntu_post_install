# name: Java - Oracle

log installing Oracle Java SE ...
TEMP_DEB="$(mktemp)" && 
  wget -q -O "$TEMP_DEB" -c --no-cookies --no-check-certificate \
        --header "Cookie: oraclelicense=accept-securebackup-cookie" \
        "${JAVA_SE_URL:?not set}" && \
sudo dpkg -i "$TEMP_DEB"
[ $? -gt 0 ] && apt_cmd --fix-broken -y